{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "source": "~/dotfiles/fastfetch/arch-min.txt",
    "type": "file",
    "padding": {
      "top": 2,
      "right": 6,
    },
  },
  "display": {
    "separator": " ",
  },
  "modules": [
    "break",
    "break",
    {
      "type": "title",
      "keyWidth": 10,
    },
    "break",
    {
      "type": "os",
      "key": " ",
      "keyColor": "33", // = color3
    },
    {
      "type": "kernel",
      "key": " ",
      "keyColor": "33",
    },
    {
      "type": "terminal",
      "key": " ",
      "keyColor": "33",
    },
    {
      "type": "wm",
      "key": " ",
      "keyColor": "33",
    },
    {
      "type": "uptime",
      "key": " ",
      "keyColor": "33",
    },
    {
      "type": "command",
      "key": "󱦟 ",
      "keyColor": "33",
      "text": "birth_install=$(stat -c %W /); current=$(date +%s); time_progression=$((current - birth_install)); days_difference=$((time_progression / 86400)); echo $days_difference days",
    },
    {
      "type": "memory",
      "key": " ",
      "keyColor": "33",
    },
    {
      "type": "disk",
      "key": " ",
      "keyColor": "33",
    },
  ],
}
